_minios_live()
{
    local cur prev opts
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    prev="${COMP_WORDS[COMP_CWORD-1]}"
    opts="setup_host build_bootstrap build_chroot build_live build_config build_modules build_iso remove_sources"

    if [[ ${cur} == * ]]
    then
      COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
    fi

    return 0
}
complete -F _minios_live minios-live
